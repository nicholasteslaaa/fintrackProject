<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane prefHeight="288.0" prefWidth="327.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fintrack.form.uiController.EditKategoriPageController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <VBox GridPane.columnIndex="1" GridPane.rowIndex="2">
         <children>
      
                    <Label text="Limit" />
              <TextField fx:id="limit" prefWidth="300" promptText="Rp." />
         </children>
      </VBox>
   
           <HBox alignment="CENTER" spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="3">
            <Button onAction="#editKategori" prefWidth="140" style="-fx-background-color: black; -fx-text-fill: white;" text="Save" />
            <Button onAction="#deleteKategori" prefWidth="140" style="-fx-background-color: red; -fx-text-fill: white;" text="Delete" />
        </HBox>
      <Label text="Edit Kategori" GridPane.columnIndex="1" GridPane.halignment="CENTER">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <VBox GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
      
                    <Label text="Category" />
              <TextField fx:id="namaKategori" prefWidth="300" />
         </children>
      </VBox>
   </children>
</GridPane>
